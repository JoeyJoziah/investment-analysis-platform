{
  "automatic_agent_usage": {
    "enabled": true,
    "default_behavior": "auto_select_agents",
    "fallback_agent": "agent-organizer"
  },
  "trigger_keywords": {
    "python": ["python-pro"],
    "fastapi": ["python-pro", "api-designer"],
    "backend": ["python-pro"],
    "api": ["api-designer", "api-documenter"],
    "database": ["database-schema-designer", "postgres-pro"],
    "postgresql": ["postgres-pro"],
    "migration": ["database-schema-designer"],
    "schema": ["database-schema-designer"],
    "ml": ["ml-engineer"],
    "machine learning": ["ml-engineer", "ai-engineer"],
    "pytorch": ["ml-engineer"],
    "model": ["ml-engineer"],
    "financial": ["quant-analyst"],
    "trading": ["quant-analyst"],
    "stock": ["quant-analyst", "data-scientist"],
    "analysis": ["quant-analyst", "data-scientist"],
    "security": ["security-auditor"],
    "authentication": ["security-auditor"],
    "performance": ["performance-optimizer"],
    "optimization": ["performance-optimizer"],
    "test": ["test-suite-generator"],
    "testing": ["test-suite-generator", "qa-expert"],
    "pytest": ["test-suite-generator"],
    "docker": ["deployment-engineer"],
    "kubernetes": ["deployment-engineer", "cloud-architect"],
    "deployment": ["deployment-engineer"],
    "frontend": ["frontend-developer"],
    "react": ["react-pro", "frontend-developer"],
    "ui": ["ui-designer", "frontend-developer"],
    "data": ["data-scientist", "data-engineer"],
    "etl": ["data-engineer"],
    "airflow": ["data-engineer"],
    "query": ["data-scientist", "postgres-pro"],
    "sql": ["data-scientist"]
  },
  "multi_domain_triggers": [
    "new feature",
    "implement",
    "add authentication",
    "create api",
    "build system",
    "full stack",
    "end-to-end",
    "complete",
    "integrate"
  ],
  "agent_combinations": {
    "api_development": ["api-designer", "python-pro", "security-auditor", "api-documenter"],
    "ml_implementation": ["ml-engineer", "python-pro", "data-engineer"],
    "database_changes": ["database-schema-designer", "postgres-pro", "performance-optimizer"],
    "security_implementation": ["security-auditor", "python-pro", "api-designer"],
    "testing_suite": ["test-suite-generator", "python-pro", "qa-expert"],
    "performance_optimization": ["performance-optimizer", "python-pro", "postgres-pro"],
    "financial_modeling": ["quant-analyst", "data-scientist", "ml-engineer"],
    "frontend_development": ["frontend-developer", "react-pro", "ui-designer"],
    "devops_deployment": ["deployment-engineer", "cloud-architect", "security-auditor"]
  },
  "quality_assurance_rules": {
    "always_include_code_reviewer": true,
    "always_include_security_for_auth": true,
    "always_include_documenter_for_api": true,
    "minimum_coverage_agents": ["code-reviewer"]
  },
  "user_override_patterns": {
    "no_agents": ["no agents", "manual only", "no automation"],
    "specific_agent": ["use only", "only use", "just use"],
    "exclude_agent": ["no ", "exclude ", "without "],
    "manual_selection": ["manual agent", "let me choose", "custom agents"]
  }
}