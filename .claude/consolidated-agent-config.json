      "ml_engineer": {
        "source": "wshobson-agents/ml-engineer.md",
        "purpose": "Model deployment, feature engineering, PyTorch/scikit-learn optimization",
        "model": "claude-3-opus"
      },
      "data_engineer": {
        "source": "wshobson-agents/data-engineer.md",
        "purpose": "Apache Airflow DAGs, Kafka streams, data processing pipelines",
        "model": "claude-3-sonnet"
      },
      "database_optimizer": {
        "source": "wshobson-agents/database-optimizer.md",
