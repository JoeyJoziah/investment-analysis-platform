{
  "project": "Investment Analysis Platform",
  "description": "Optimized agent configuration for financial analysis, ML/AI, and data pipeline operations",
  
  "primary_agents": {
    "financial_core": {
      "trading_framework": {
        "source": "TradingAgents",
        "agents": [
          "fundamental_analyst",
          "sentiment_analyst", 
          "news_analyst",
          "technical_analyst",
          "bull_researcher",
          "bear_researcher",
          "portfolio_manager"
        ],
        "purpose": "Core financial analysis and trading logic with agent debates"
      },
      "quant_analyst": {
        "source": "wshobson-agents/quant-analyst.md",
        "purpose": "Financial modeling, backtesting, R-multiple analysis",
        "model": "claude-3-opus"
      },
      "risk_manager": {
        "source": "wshobson-agents/risk-manager.md",
        "purpose": "Portfolio risk assessment, position sizing, expectancy calculation",
        "model": "claude-3-sonnet"
      },
      "fintech_engineer": {
        "source": "voltagent-subagents/fintech-engineer.md",
        "purpose": "SEC/GDPR compliance, regulatory requirements, payment processing"
