{
  "dashboard": {
    "id": null,
    "uid": "investment-system-health",
    "title": "Investment Analysis - System Health",
    "description": "Comprehensive system health monitoring dashboard",
    "tags": ["investment-analysis", "system", "health", "infrastructure"],
    "timezone": "browser",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "app_uptime_seconds",
            "legendFormat": "Uptime (hours)",
            "refId": "A"
          },
          {
            "expr": "cpu_usage_percent", 
            "legendFormat": "CPU Usage %",
            "refId": "B"
          },
          {
            "expr": "memory_usage_percent",
            "legendFormat": "Memory Usage %", 
            "refId": "C"
          },
          {
            "expr": "disk_usage_percent",
            "legendFormat": "Disk Usage %",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 85}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Uptime (hours)"},
              "properties": [
                {"id": "unit", "value": "s"},
                {"id": "custom.displayMode", "value": "lcd-gauge"}
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 2,
        "title": "API Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "Requests/sec - {{method}} {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(api_latency_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency - {{endpoint}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 3,
        "title": "Database Performance",
        "type": "timeseries", 
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "database_connections{state=\"active\"}",
            "legendFormat": "Active Connections - {{database}}",
            "refId": "A"
          },
          {
            "expr": "postgres_cache_hit_ratio",
            "legendFormat": "Cache Hit Ratio % - {{database}}",
            "refId": "B"
          },
          {
            "expr": "rate(database_query_duration_seconds_sum[5m]) / rate(database_query_duration_seconds_count[5m])",
            "legendFormat": "Avg Query Duration - {{operation}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 4,
        "title": "Cache Performance",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Ratio % - {{cache_type}}",
            "refId": "A"
          },
          {
            "expr": "cache_size_entries",
            "legendFormat": "Entries - {{cache_type}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 85}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 5,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 16},
        "targets": [
          {
            "expr": "circuit_breaker_state",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [
              {"options": {"0": {"text": "CLOSED", "color": "green"}}, "type": "value"},
              {"options": {"1": {"text": "OPEN", "color": "red"}}, "type": "value"},
              {"options": {"2": {"text": "HALF-OPEN", "color": "yellow"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "name",
          "colorMode": "background"
        }
      },
      {
        "id": 6,
        "title": "Active Tasks & Threads",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "active_tasks",
            "legendFormat": "Active Async Tasks",
            "refId": "A"
          },
          {
            "expr": "thread_pool_active",
            "legendFormat": "Active Threads - {{pool_name}}",
            "refId": "B"
          },
          {
            "expr": "concurrent_requests",
            "legendFormat": "Concurrent Requests",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 7,
        "title": "Error Rates",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "rate(api_errors_total[5m])",
            "legendFormat": "API Errors/sec - {{error_type}}",
            "refId": "A"
          },
          {
            "expr": "rate(database_errors_total[5m])",
            "legendFormat": "DB Errors/sec - {{operation}}",
            "refId": "B"
          },
          {
            "expr": "rate(stock_processing_errors_total[5m])",
            "legendFormat": "Stock Processing Errors/sec - {{tier}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 0.2,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "normal", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "color": {"mode": "palette-classic", "fixedColor": "red"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 8,
        "title": "Network & I/O",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "rate(network_bytes_sent_total[5m])",
            "legendFormat": "Bytes Sent/sec - {{interface}}",
            "refId": "A"
          },
          {
            "expr": "rate(network_bytes_received_total[5m])",
            "legendFormat": "Bytes Received/sec - {{interface}}",
            "refId": "B"
          },
          {
            "expr": "open_connections",
            "legendFormat": "Open Connections",
            "refId": "C"
          },
          {
            "expr": "file_descriptors_open",
            "legendFormat": "Open File Descriptors",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "binBps"
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*Connections.*|.*Descriptors.*"},
              "properties": [{"id": "unit", "value": "short"}]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "label": "Data Source",
          "type": "datasource",
          "query": "prometheus",
          "current": {
