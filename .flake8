[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    docs/source/conf.py,
    old,
    build,
    dist,
    .eggs,
    *.egg,
    .venv,
    venv,
    migrations,
    .tox,
    .mypy_cache,
    .pytest_cache,
    .hypothesis
per-file-ignores =
    __init__.py:F401
max-complexity = 10