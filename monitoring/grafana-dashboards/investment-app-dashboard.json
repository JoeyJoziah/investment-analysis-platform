{
  "dashboard": {
    "id": null,
    "uid": "investment-app",
    "title": "Investment Analysis App Dashboard",
    "tags": ["investment", "monitoring", "production"],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "API Request Rate",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "API Latency (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(api_latency_seconds_bucket[5m]))",
            "legendFormat": "p99 {{endpoint}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "graph",
        "title": "Database Connections",
        "targets": [
          {
            "expr": "database_connections",
            "legendFormat": "{{database}} - {{state}}",
            "refId": "A"
          },
          {
            "expr": "database_pool_size",
            "legendFormat": "Pool Size",
            "refId": "B"
          }
        ]
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "graph",
        "title": "Cache Performance",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Hits {{cache_type}}",
            "refId": "A"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Misses {{cache_type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "stat",
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Cost Tracking",
        "targets": [
          {
            "expr": "estimated_monthly_cost_usd",
            "legendFormat": "Estimated Monthly Cost",
            "refId": "A"
          },
          {
            "expr": "50",
            "legendFormat": "Budget Limit",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "show": true},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [45],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "Cost Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "API Calls Remaining",
        "targets": [
          {
            "expr": "api_calls_remaining",
            "legendFormat": "{{provider}} - {{limit_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
        "type": "graph",
        "title": "Stock Processing",
        "targets": [
          {
            "expr": "rate(stocks_processed_total[5m])",
            "legendFormat": "{{tier}} - {{data_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24},
        "type": "stat",
        "title": "Stocks by Tier",
        "targets": [
          {
            "expr": "stocks_in_tier",
            "legendFormat": "{{tier}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24},
        "type": "gauge",
        "title": "Circuit Breaker Status",
        "targets": [
          {
            "expr": "circuit_breaker_state{service=\"redis\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "Closed"},
              {"type": "value", "value": "1", "text": "Open"},
              {"type": "value", "value": "2", "text": "Half-Open"}
            ]
          }
        }
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "type": "graph",
        "title": "System Resources",
        "targets": [
          {
            "expr": "cpu_usage_percent",
            "legendFormat": "CPU %",
            "refId": "A"
          },
          {
            "expr": "memory_usage_percent",
            "legendFormat": "Memory %",
            "refId": "B"
          },
          {
            "expr": "disk_usage_percent",
            "legendFormat": "Disk %",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "percent", "show": true, "max": 100},
          {"show": false}
        ]
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "type": "graph",
        "title": "Error Rates",
        "targets": [
          {
            "expr": "rate(api_errors_total[5m])",
            "legendFormat": "API Errors - {{error_type}}",
            "refId": "A"
          },
          {
            "expr": "rate(database_errors_total[5m])",
            "legendFormat": "DB Errors - {{error_type}}",
            "refId": "B"
          },
          {
            "expr": "rate(stock_processing_errors_total[5m])",
            "legendFormat": "Processing Errors - {{tier}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ops", "show": true},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.1],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40},
        "type": "table",
        "title": "Model Performance",
        "targets": [
          {
            "expr": "model_accuracy",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "rate(model_predictions_total[5m])",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(model_inference_seconds_bucket[5m]))",
            "format": "table",
            "instant": true,
            "refId": "C"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        },
        {
          "name": "refresh_interval",
          "type": "interval",
          "query": "5s,10s,30s,1m,5m",
          "current": {
            "text": "30s",
            "value": "30s"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "name": "Alerts",
          "step": "60s",
          "tagKeys": "alertname",
          "titleFormat": "{{ alertname }}",
          "type": "alert"
        }
      ]
    }
  }
}